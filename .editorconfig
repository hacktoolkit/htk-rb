# See http://EditorConfig.org for details

# Top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Two space indent by default
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4

[*.rb]
indent_size = 2

[*.gemspec]
indent_size = 2
